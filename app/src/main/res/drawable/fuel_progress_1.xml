<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/segment_1">
        <rotate android:fromDegrees="0" android:toDegrees="55">
            <shape
                android:shape="oval">
                <stroke android:color="@color/colorPrimary" android:width="@dimen/_5sdp" />
            </shape>
        </rotate>
    </item>
    <item android:id="@+id/segment_2">
        <rotate android:fromDegrees="60" android:toDegrees="115">
            <shape
                android:shape="oval">
                <stroke android:color="@color/colorPrimary" android:width="@dimen/_5sdp" />
            </shape>
        </rotate>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:color="@color/gray" android:width="@dimen/_5sdp" />
        </shape>
    </item>
</layer-list>